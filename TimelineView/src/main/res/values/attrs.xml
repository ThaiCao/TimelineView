<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimelineView">
        <attr name="timeline_item_type" format="enum">
            <enum name="first" value="0"/>
            <enum name="middle" value="1"/>
            <enum name="last" value="2"/>
            <enum name="spacer" value="3"/>
        </attr>
        <attr name="indicator_radius" format="dimension"/>
        <attr name="line_width" format="dimension"/>
        <attr name="indicator_color" format="color"/>
        <attr name="line_color" format="color"/>
    </declare-styleable>
</resources>